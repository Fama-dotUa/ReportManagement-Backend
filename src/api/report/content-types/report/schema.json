{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "singularName": "report",
    "pluralName": "reports",
    "displayName": "report"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "maxLength": 45,
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "time_to_free": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 30
    },
    "reason": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::reason.reason"
    }
  }
}
