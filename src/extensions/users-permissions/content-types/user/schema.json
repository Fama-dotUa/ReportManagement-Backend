{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "discord": {
      "type": "string",
      "minLength": 2,
      "maxLength": 37,
      "required": true,
      "unique": true,
      "regex": "^([a-zA-Z0-9_.]{2,32}|[a-zA-Z0-9_]{2,32}#[0-9]{4})$"
    },
    "Icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "rank": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rank.rank"
    },
    "reports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::report.report",
      "mappedBy": "user"
    },
    "Description": {
      "type": "richtext"
    },
    "CR": {
      "type": "biginteger",
      "default": "0"
    },
    "positions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::position.position"
    },
    "fon_schildiks_all": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::fon-schildik.fon-schildik"
    },
    "fon_schildik_active": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::fon-schildik.fon-schildik"
    },
    "framesfor_avatars_all": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::framesfor-avatar.framesfor-avatar"
    },
    "framesfor_avatar_active": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::framesfor-avatar.framesfor-avatar"
    },
    "profile_backgrounds_all": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::profile-background.profile-background"
    },
    "profile_background_active": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::profile-background.profile-background"
    },
    "CR_for_all_time": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "last_login": {
      "type": "datetime"
    }
  }
}
